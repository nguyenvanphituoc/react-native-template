{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:ios:dev": "bundle exec fastlane ios delivery_ipa --env ios.vinova",
    "build:android:dev": "bundle exec fastlane android delivery_apk --env android.vinova",
    "release:android:dev": "./scripts/bin/gitlab_trigger",
    "release:ios:dev": "./scripts/bin/bitrise_trigger",
    "preinstall": "./scripts/preinstall",
    "postinstall": "./scripts/postinstall",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "NODE_ENV=development react-native start",
    "test": "jest && node ./scripts/jest/coverage.mjs",
    "clean": "./scripts/clean --keep-env",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "csv-parser": "^3.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "lint-staged": "^15.2.2",
    "patch-package": "^8.0.0",
    "plist": "^3.1.0",
    "prettier": "2.8.8",
    "react-dom": "^18.2.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "5.0.4",
    "xcode": "^3.0.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,mjs,json}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "engines": {
    "node": ">=18"
  }
}
