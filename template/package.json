    "template",
    "template.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguyenvanphituoc/react-native-template.git"
  },
  "keywords": [
    "react-native",
    "typescript",
    "jest",
    "template",
    "boilerplate"
  ],
  "author": "LibertyNguyen <nguyenvanphituoc.developer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nguyenvanphituoc/react-native-template/issues"
  },
  "homepage": "https://github.com/nguyenvanphituoc/react-native-template",
  "dependencies": {
    "release-it": "^17.1.1"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.1"
  }
}
